{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "react/no-multi-comp": 0,
    "func-names": 0,
    "object-curly-spacing": 0,
    "max-len": 0,
    "quote-props": 0,
    "prefer-template": 0,
    "no-alert": 0,

    "arrow-body-style": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-indent-props": 0,
    "no-param-reassign": 0,
    "object-shorthand": 1,
    "react/jsx-no-bind": 1,

    "no-debugger": 2,
  }
}
